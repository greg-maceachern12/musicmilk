// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mix {
  id          String   @id @default(cuid())
  blobName    String   @unique
  title       String
  artist      String?
  genre       String?
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}